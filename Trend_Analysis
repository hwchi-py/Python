## trend
d=damean_twsa
f=frmean_twsa
g=jpl
d=np.nan_to_num(d,0)
f=np.nan_to_num(f,0)
g=np.nan_to_num(g,0) 
d_trend=np.zeros((132,220))
f_trend=np.zeros((132,220))
g_trend=np.zeros((132,220))
for aa in range(0,132):
    for bb in range(0,220):
        g_trend[aa,bb]=mk.original_test(g[:,aa,bb]).slope
        d_trend[aa,bb]=mk.original_test(d[:,aa,bb]).slope
        f_trend[aa,bb]=mk.original_test(f[:,aa,bb]).slope
d_trend[d_trend==0]=np.nan
f_trend[f_trend==0]=np.nan
g_trend[g_trend==0]=np.nan
